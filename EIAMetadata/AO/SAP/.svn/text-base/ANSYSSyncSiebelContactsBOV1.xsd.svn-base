		<xsd:sequence>
			<xsd:element name="Name" maxOccurs="1" minOccurs="1" type="xsd:string"/>
			<xsd:element name="Description" maxOccurs="1" minOccurs="0" type="xsd:string"/>
			<xsd:element name="Alias" maxOccurs="1" minOccurs="0" type="xsd:string"/>
