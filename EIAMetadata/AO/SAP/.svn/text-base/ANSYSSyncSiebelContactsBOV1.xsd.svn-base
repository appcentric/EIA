	<xsd:element name="ANSYSSiebelContact_ResponseMessageType">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GmailAccountId" type="xsd:string" minOccurs="1"/>
				<xsd:element name="GmailAccountPwd" type="xsd:string" minOccurs="1"/>
				<xsd:element name="SiebelUserLogin" type="xsd:string"/>
