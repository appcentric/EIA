<xsd:simpleType name="PostingMonthType">
<xsd:restriction base="xsd:string">
<xsd:pattern value="((01|02|03|04|05|06|07|08|09|10|11|12)-([0-9][0-9]))"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="Date6">
<xsd:annotation>
<xsd:documentation>YYMMDD Format Date</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:minLength value="0"/>
<xsd:maxLength value="6"/>
<xsd:pattern value="[0-9]+"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="Time4">
<xsd:annotation>
<xsd:documentation>Time of day in Military Format, 24 hours</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:minLength value="0"/>
<xsd:maxLength value="4"/>
<xsd:pattern value="[0-9]+"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="N1">
<xsd:annotation>
<xsd:documentation>Numeric, 1 digit</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:minLength value="0"/>
<xsd:maxLength value="1"/>
<xsd:pattern value="[0-9]+"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="N3">
<xsd:annotation>
<xsd:documentation>Numeric, 3 digit</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:minLength value="0"/>
<xsd:maxLength value="3"/>
<xsd:pattern value="[0-9]+"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="IntegerNoDecimal">
<xsd:annotation>
<xsd:documentation>Integer value without decimal</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="Integer">
<xsd:fractionDigits value="0"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="DataTypeEnum">
<xsd:restriction base="C">
<xsd:enumeration value="CostCentres"/>
<xsd:enumeration value="Accounts"/>
<xsd:enumeration value="Information"/>
<xsd:enumeration value="Suppliers"/>
<xsd:enumeration value="Invoices"/>
<xsd:enumeration value="PaidInvoices"/>
</xsd:restriction>
</xsd:simpleType>
