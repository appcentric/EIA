<xsd:simpleType name="Boolean">
<xsd:annotation>
<xsd:documentation>The base type for LSC's boolean adaptations</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:boolean"/>
</xsd:simpleType>
<xsd:simpleType name="BooleanNumeric">
<xsd:annotation>
<xsd:documentation>1 and 0 implementation of boolean</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="Boolean">
<xsd:pattern value="0"/>
<xsd:pattern value="1"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="BooleanString">
<xsd:annotation>
<xsd:documentation>string implementation of boolean</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="Boolean">
<xsd:pattern value="true"/>
<xsd:pattern value="false"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="Dt">
<xsd:annotation>
<xsd:documentation>A simpleType for a particular day-date</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:date"/>
</xsd:simpleType>
<xsd:simpleType name="Tm">
<xsd:annotation>
<xsd:documentation>A simpleType for a particular time</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:time"/>
</xsd:simpleType>
<xsd:simpleType name="DtTm">
<xsd:annotation>
<xsd:documentation>A simpleType for a date and time useful for timestamps
        </xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:dateTime"/>
</xsd:simpleType>
<xsd:simpleType name="Year">
<xsd:annotation>
<xsd:documentation>A simpleType for a year
        </xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:gYear"/>
</xsd:simpleType>
<xsd:simpleType name="PhoneNum">
<xsd:annotation>
<xsd:documentation>A simple type for use with phone numbers</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:maxLength value="15"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="EmailAddress">
<xsd:annotation>
<xsd:documentation>A simple type for use with email address</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C">
<xsd:maxLength value="20"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="Flag">
<xsd:annotation>
<xsd:documentation>The base type for LBH's flag adaptations</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:boolean"/>
</xsd:simpleType>
<xsd:simpleType name="Decimal">
<xsd:annotation>
<xsd:documentation>A rename of the Decimal data type</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:decimal"/>
</xsd:simpleType>
<xsd:simpleType name="Long">
<xsd:annotation>
<xsd:documentation>Long is a 64-bit integer value, which may be positive, negative, or zero.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:long"/>
</xsd:simpleType>
<xsd:simpleType name="Integer">
<xsd:annotation>
<xsd:documentation> The rename of the integer type, which may be positive, negative, or zero.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:integer"/>
</xsd:simpleType>
<xsd:simpleType name="PositiveInteger">
<xsd:annotation>
<xsd:documentation> A positive integer to represent Id's of all entities.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:nonNegativeInteger">
<xsd:minInclusive value="1"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="AccountNumber">
<xsd:annotation>
<xsd:documentation> The rename of the integer type, which may be positive, negative, or zero.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:positiveInteger"/>
</xsd:simpleType>
<xsd:simpleType name="MonetaryAmt">
<xsd:annotation>
<xsd:documentation>A 2 decimal place means of expressing monetary amounts</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="Decimal">
<xsd:fractionDigits value="2"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="Amount">
<xsd:annotation>
<xsd:documentation>A 2 decimal place means of expressing  amounts</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="Decimal">
<xsd:fractionDigits value="2"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="ClosedEnum">
<xsd:annotation>
<xsd:documentation>The base type for a closed string xsd:enumeration i.e. one not expected to change</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C"/>
</xsd:simpleType>
<xsd:simpleType name="OpenEnum">
<xsd:annotation>
<xsd:documentation>The base type for an open string xsd:enumeration i.e. one which is likely to have values added</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="C"/>
</xsd:simpleType>
<xsd:complexType name="Bin">
<xsd:annotation>
<xsd:documentation>The base type for Binaries</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="BinContentType" type="BinContentType">
<xsd:annotation>
<xsd:documentation>Enumeration based on MIME types.</xsd:documentation>
