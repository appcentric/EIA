	<!-- BEGIN Assembly_Party *****************************************************************-->
	<xsd:complexType name="Assembly_Party">
		<xsd:annotation>
			<xsd:documentation>
				This is the Party_Assembly.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="cica:Text_PartyName" minOccurs="0" maxOccurs="5"/>
			<!-- Identifier used to specify a particular Nucor Division or Customer location -->
			<xsd:element name="Identifier" type="cica:Code_Indentifier" minOccurs="1" maxOccurs="5"/>
			<xsd:element name="PhysicalLocation" type="cica:Component_PhysicalLocation" minOccurs="0" maxOccurs="3"/>
			<xsd:element name="Contact" type="cica:Block_Contact" minOccurs="0" maxOccurs="10"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_Party" use="required"/>
	</xsd:complexType>

	<xsd:simpleType name="Number_Line">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Code_Party">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="0"/>
			<xsd:enumeration value="SU"/> <!--Supplier/Manufacturer-->
			<xsd:enumeration value="ST"/> <!--Ship TO-->
			<xsd:enumeration value="SE"/> <!--Selling-->
			<xsd:enumeration value="SF"/> <!--Ship FROM-->
			<xsd:enumeration value="AC"/> <!--Air Cargo Company-->
			<xsd:enumeration value="MA"/> <!--Party for whom Item is Ultimately Intended-->
			<xsd:enumeration value="BD"/> <!--Bill-to Party for Diversion Charges-->
			<xsd:enumeration value="BT"/> <!--Bill TO-->
			<xsd:enumeration value="BS"/> <!--Bill TO Party AND Ship TO party-->
			<xsd:enumeration value="RI"/> <!--Remit TO-->
			<xsd:enumeration value="RC"/> <!--Receiving Location-->
			<xsd:enumeration value="VN"/> <!--Vendor-->
			<xsd:enumeration value="8P"/> <!--Producer-->
			<xsd:enumeration value="BY"/> <!--Buying party or Purchaser-->
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>


