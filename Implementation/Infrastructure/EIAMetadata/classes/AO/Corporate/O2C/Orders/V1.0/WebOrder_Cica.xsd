	<xsd:complexType name="Component_TermsOfSale">
		<xsd:annotation>
			<xsd:documentation>This is a TermsOfSale_Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DiscountPercent" type="cica:Numeric_DiscountPercent" minOccurs="0"/>
			<xsd:element name="DiscountDays" type="cica:Numeric_DiscountDays" minOccurs="0"/>
			<xsd:element name="NetDays" type="cica:Numeric_NetDays" minOccurs="0"/>
			<xsd:element name="Desc" type="cica:Text_TermsOfSaleDesc" minOccurs="0"/>
			<xsd:element name="DiscountDateDue" type="cica:Text_TermsOfSaleDateDue" minOccurs="0"/>
			<xsd:element name="NetDateDue" type="cica:Text_TermsOfSaleDateDue" minOccurs="0"/>
			<xsd:element name="DiscountAmount" type="cica:Amount_TermsOfSaleDiscountAmount" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_TermsOfSaleTypeCode" use="required"/>
		<xsd:attribute name="BasisDateCode" type="cica:Code_TermsOfSaleBasisDate" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="Numeric_DiscountPercent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="Numeric_DiscountDays">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="Numeric_NetDays">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="Text_TermsOfSaleDesc">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_TermsOfSaleDateDue">
		<xsd:restriction base="xsd:dateTime"/>
	</xsd:simpleType>
	<xsd:simpleType name="Code_TermsOfSaleTypeCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="01"/>
			<xsd:enumeration value="02"/>
			<xsd:enumeration value="08"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_TermsOfSaleBasisDate">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="01"/>
			<xsd:enumeration value="02"/>
			<xsd:enumeration value="03"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- BEGIN PRIMITIVE_Code_ForecastTypeCode *********************************************-->
	<xsd:simpleType name="Code_ForecastTypeCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="1"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="A"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="D"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Code_ForecastTypeCode *****************************************************-->
	<!-- BEGIN PRIMITIVE_Amount_TermsOfSaleDiscountAmount *********************************************-->
	<xsd:complexType name="Amount_TermsOfSaleDiscountAmount">
		<xsd:simpleContent>
			<xsd:extension base="cica:Amount_TermsOfSaleDiscountAmountContent">
				<xsd:attribute name="CurrencyCode" type="cica:Content_CurrencyCode" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Amount_TermsOfSaleDiscountAmountContent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Amount_TermsOfSaleDiscountAmount  *****************************************************-->
	<!-- END Component_TermsOfSale *****************************************************************-->


	<!-- BEGIN Component_Transportation *****************************************************************-->
	<xsd:complexType name="Component_Transportation">
		<xsd:annotation>
			<xsd:documentation>This is a Transportation_Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Point" type="cica:Text_TransportPoint" minOccurs="0" maxOccurs="7"/>
		</xsd:sequence>
		<xsd:attribute name="MethodOfPayment" type="cica:Code_MethodOfPayment" use="required"/>
		<xsd:attribute name="TermsCode" type="cica:Code_TermsCode" use="required"/>
		<xsd:attribute name="TermsQual" type="cica:Code_TermsQual" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="Text_TransportPoint">
		<xsd:simpleContent>
			<xsd:extension base="cica:Content_Text_TransportPoint">
				<xsd:attribute name="TypeCode" type="cica:Content_TransportPointTextTypeCode" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Content_Text_TransportPoint">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Content_TransportPointTextTypeCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="CI"/>
			<xsd:enumeration value="DE"/>
			<xsd:enumeration value="PL"/>
			<xsd:enumeration value="FOB"/>
			<xsd:enumeration value="OR"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="Text_DestinationPoint">
		<xsd:simpleContent>
			<xsd:extension base="cica:Content_DestinationPointText">
				<xsd:attribute name="TypeCode" type="cica:Content_DestinationPointText" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Content_DestinationPointText">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Content_DestinationPointTextTypeCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="OA"/>
			<xsd:enumeration value="FOB"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_TermsQual">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="02"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_TermsCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="FOB"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_MethodOfPayment">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="CC"/> <!--Collect-->
			<xsd:enumeration value="BP"/> <!--Buyer Paid-->
			<xsd:enumeration value="PP"/> <!--Prepaid NOTE for Nucor use PC and PP are the same-->
			<xsd:enumeration value="PC"/> <!--Prepaid but charged to customer-->
			<xsd:enumeration value="PA"/> <!--Prepaid advance by seller-->
			<xsd:enumeration value="PD"/> <!--Prepaid by seller customer not charged-->
			<xsd:enumeration value="PU"/> <!--Pickup-->
			<xsd:enumeration value="DF"/> <!--Defined by Buyer and Seller-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END Component_Transportation *****************************************************************-->


	<!-- BEGIN Component_Carriage *****************************************************************-->
	<xsd:complexType name="Component_Carriage">
		<xsd:annotation>
			<xsd:documentation>This is a Carriage_Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Carrier" type="cica:Text_CarrierName" minOccurs="0"/>
			<xsd:element name="Desc" type="cica:Text_CarrierDesc" minOccurs="0"/>
			<xsd:element name="EquipmentCode" type="cica:Code_Equipment" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="MethodCode" type="cica:Code_MethodCode" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="Text_CarrierName">
		<xsd:simpleContent>
			<xsd:extension base="cica:Content_CarrierText">
				<xsd:attribute name="TypeCode" type="cica:Content_CarrierTextTypeCode" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Content_CarrierText">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Content_CarrierTextTypeCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="91"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_CarrierDesc">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_MethodCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="A"/><!--Air-->
			<xsd:enumeration value="B"/><!--Barge-->
			<xsd:enumeration value="H"/><!--Customer Pickup-->
			<xsd:enumeration value="I"/><!--Common Irregular Carrier-->
			<xsd:enumeration value="J"/><!--Motor-->
			<xsd:enumeration value="L"/><!--Contract Carrier-->
			<xsd:enumeration value="M"/><!--Motor (Common Carrier)-->
			<xsd:enumeration value="P"/><!--Private Carrier-->
			<xsd:enumeration value="R"/><!--Rail-->
			<xsd:enumeration value="S"/><!--Ocean-->
			<xsd:enumeration value="CE"/><!--Customer Pickup / Customer's Expense-->
			<xsd:enumeration value="FL"/><!--Motor (Flatbed)-->
			<xsd:enumeration value="SC"/><!--Shipper Agent (Truck)-->		
			<xsd:enumeration value="SR"/><!--Supplier Truck-->		
			<xsd:enumeration value="VE"/><!--Vessel, Ocean-->
			<xsd:enumeration value="RR"/><!--Rail/Truck-->
			<xsd:enumeration value="ZZ"/><!--Transload-->	
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_Equipment">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="TR"/>	<!--Truck-->
			<xsd:enumeration value="RR"/>	<!--Rail-->
			<xsd:enumeration value="BR"/>	<!--Barge-->
			<xsd:enumeration value="VE"/>	<!--Ocean Vessel-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END Component_Carriage *****************************************************************-->
	<!-- BEGIN Component_PhysicalLocation *****************************************************************-->
	<xsd:complexType name="Component_PhysicalLocation">
		<xsd:annotation>
			<xsd:documentation>This is an Physical Location Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Address" type="cica:Text_Address" minOccurs="0" maxOccurs="2"/>
			<xsd:element name="Address2" type="cica:Text_Address" minOccurs="0" maxOccurs="2"/>
			<xsd:element name="City" type="cica:Text_City" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="State" type="cica:Text_State" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="PostalCode" type="cica:Text_PostalCode" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Country" type="cica:Text_Country" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_PhysicalLocation" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="Text_Address">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_City">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_State">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_PostalCode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="15"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_Country">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_PhysicalLocation">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="2"/>
			<xsd:enumeration value="PL"/>  	<!-- Physical site -->
			<xsd:enumeration value="PC"/>	<!-- Physical address -->
			<xsd:enumeration value="UN"/>	<!-- Unknown -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END Component_PhysicalLocation *****************************************************************-->
	<!--BEGIN Component_IndividualName******************************************************-->
	<xsd:complexType name="Component_IndividualName">
		<xsd:annotation>
			<xsd:documentation>
				This is the Name component for people.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Prefix" type="cica:Text_Prefix" minOccurs="0"/>
			<xsd:element name="GivenName" type="cica:Text_GivenName" minOccurs="0"/>
			<xsd:element name="MiddleName" type="cica:Text_MiddleName" minOccurs="0"/>
			<xsd:element name="SurName" type="cica:Text_SurName" minOccurs="0"/>
			<xsd:element name="Suffix" type="cica:Text_Suffix" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_Contact" use="optional"/>
	</xsd:complexType>
	<xsd:simpleType name="Text_Prefix">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_GivenName">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_MiddleName">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_SurName">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text_Suffix">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code_Contact">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="AC"/>	<!-- Admin Contracting Officer (rare) -->
			<xsd:enumeration value="BD"/>	<!-- Buyer -->
			<xsd:enumeration value="SD"/>	<!-- Shipping -->
			<xsd:enumeration value="OC"/>	<!-- Sales/Order -->
			<xsd:enumeration value="RE"/>	<!-- Receiving -->
			<xsd:enumeration value="SH"/>	<!-- Shipper/Carrier -->
			<xsd:enumeration value="AR"/>	<!-- Accounts Receivable -->
			<xsd:enumeration value="SA"/>	<!-- Sales Administration -->
		</xsd:restriction>
	</xsd:simpleType>
	<!--END Component_IndividualName******************************************************-->
	<!--BEGIN Component_Dimensions****************************************************-->
	<xsd:complexType name="Component_Dimensions">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Dimensions Component</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="Count" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Length" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Width" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Height" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="OutsideDia" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="InsideDia" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Diameter" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Gauge" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Weight" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Leg1" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Leg2" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="Thickness" type="cica:Measure_Linear" minOccurs="0"/>
			<xsd:element name="WeightPerFoot" type="cica:Measure_Linear" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="Measure_Linear">
		<xsd:simpleContent>
			<xsd:extension base="cica:Content_Measure_Linear">
				<xsd:attribute name="UOM" type="cica:Code_UOM" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Content_Measure_Linear">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!--END Component_Dimensions******************************************************-->
	<!--BEGIN Component_ScheduleRequest***********************************************-->
	<xsd:complexType name="Component_ScheduleRequest">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Schedule Request Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DateRequested" type="cica:DateTime_Requested"/>
			<xsd:element name="Quantity" type="cica:Quantity_Quantity"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="DateTime_Requested">
		<xsd:restriction base="xsd:dateTime"/>
	</xsd:simpleType>
	<!--END Component_ScheduleRequest*************************************************-->
	<!--BEGIN Component_Specification*************************************************-->
	<xsd:complexType name="Component_Specification">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Specification Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AgencyCode" type="cica:Code_Agency" minOccurs="0"/>
			<xsd:element name="AgencyServiceCode" type="cica:Code_AgencyService" minOccurs="0"/>
			<xsd:element name="ChargeRate" type="cica:Amount_ChargeRate" minOccurs="0"/>
			<xsd:element name="TotalCharge" type="cica:Amount_TotalCharge" minOccurs="0"/>
			<xsd:element name="TotalWeight" type="cica:Measure_TotalWeight" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_Spec" use="required"/>
		<xsd:attribute name="ChargeCode" type="cica:Code_Charge" use="required"/>
	</xsd:complexType>
	<!--END Component_Specification**************************************************-->



	<!--BEGIN Component_Pack*************************************************-->
	<xsd:complexType name="Component_Pack">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Pack Component</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Reference" type="cica:Identifier_Reference" maxOccurs="5"/>
			<xsd:element name="BundleWeight" type="cica:Measure_BundleWeight" minOccurs="0"/>
			<xsd:element name="BundlePieces" type="cica:Quantity_BundlePieces" minOccurs="0"/>
			<xsd:element name="CountryOfMelted" type="cica:Code_Country" minOccurs="0"/>
			<xsd:element name="CountryOfManufactured" type="cica:Code_Country" minOccurs="0"/>
			<xsd:element name="DateMelted" type="cica:Date_Manufactured" minOccurs="0"/>
			<xsd:element name="DateRolled" type="cica:Date_Manufactured" minOccurs="0"/>
      			<xsd:element name="SpecComments" type="cica:Assembly_SpecComments" minOccurs="0" maxOccurs="unbounded"/>
      			<xsd:element name="MillTestResults" type="cica:Assembly_MillTestResults" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="Number" type="cica:Identifier_Number" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="Identifier_Number">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="Date_Manufactured">
		<xsd:restriction base="xsd:dateTime"/>
	</xsd:simpleType>
	<!--END Component_Pack**************************************************-->


	<!-- BEGIN PRIMITIVE_Quantity_BundlePieces  *************************************************-->
	<xsd:complexType name="Quantity_BundlePieces">
		<xsd:simpleContent>
			<xsd:extension base="cica:Quantity_BundlePiecesContent"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Quantity_BundlePiecesContent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Quantity_BundlePieces  **************************************************-->


	<!-- BEGIN PRIMITIVE_Measure_TotalWeight  *************************************************-->
	<xsd:complexType name="Measure_TotalWeight">
		<xsd:simpleContent>
			<xsd:extension base="cica:Measure_TotalWeightContent">
				<xsd:attribute name="UOM" type="cica:Code_UOM" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Measure_TotalWeightContent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Measure_TotalWeight  **************************************************-->


	<!-- BEGIN PRIMITIVE_Measure_BundleWeight  *************************************************-->
	<xsd:complexType name="Measure_BundleWeight">
		<xsd:simpleContent>
			<xsd:extension base="cica:Measure_BundleWeightContent">
				<xsd:attribute name="UOM" type="cica:Code_UOM" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Measure_BundleWeightContent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Measure_BundleWeight  **************************************************-->


	<!-- BEGIN PRIMITIVE_Text_MeasurmentDesc *************************************************-->
	<xsd:simpleType name="Text_MeasurmentDesc">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Text_MeasurmentDesc  **************************************************-->

	<!-- BEGIN PRIMITIVE_Text_ProductDesc *************************************************-->
	<xsd:simpleType name="Text_ProductDesc">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Text_ProductDesc  **************************************************-->


	<!-- BEGIN PRIMITIVE_Text_ProductSize *************************************************-->
	<xsd:simpleType name="Text_ProductSize">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Text_ProductSize  **************************************************-->
	<!-- BEGIN PRIMITIVE_Text_ProductGrade *************************************************-->
	<xsd:simpleType name="Text_ProductGrade">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Text_ProductGrade  **************************************************-->
	<!-- BEGIN PRIMITIVE_Amount_TotalCharge  ***************************************************-->
	<xsd:complexType name="Amount_TotalCharge">
		<xsd:simpleContent>
			<xsd:extension base="cica:Amount_TotalChargeContent">
				<xsd:attribute name="CurrencyCode" type="cica:Content_CurrencyCode" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Amount_TotalChargeContent">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- END PRIMITIVE_Amount_TotalCharge  *****************************************************-->
