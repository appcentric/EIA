	<!-- BEGIN Block_Contact *****************************************************************-->
	<xsd:complexType name="Block_Contact">
		<xsd:annotation>
			<xsd:documentation>
				This is the Contact_Block schema
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="cica:Component_IndividualName" minOccurs="0"/>
			<xsd:element name="JobTitle" type="cica:Text_JobTitle" minOccurs="0"/>
			<xsd:element name="PhysicalLocation" type="cica:Component_PhysicalLocation" minOccurs="0" maxOccurs="3"/>
			<xsd:element name="ElectronicLocation" type="cica:Text_ElectronicLocation" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="TypeCode" type="cica:Code_Contact" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="Text_JobTitle">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="Text_ElectronicLocation">
		<xsd:simpleContent>
			<xsd:extension base="cica:Content_ElectronicLocationText">
				<xsd:attribute name="TypeCode" type="cica:Content_ElectronicLocationType"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="Content_ElectronicLocationText">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Content_ElectronicLocationType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
			<xsd:enumeration value="EA"/>	<!-- Email Address-->
			<xsd:enumeration value="CP"/>	<!-- Cell phone-->
			<xsd:enumeration value="HP"/>	<!-- Home phone-->
			<xsd:enumeration value="FX"/>	<!-- Fax number-->
			<xsd:enumeration value="TE"/>	<!-- Just plain Telephone number no desc-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- END Block_Contact *****************************************************************-->


