            </faultHandlers>
            <sequence>
              <assign name="Assign_CommonLogging_PL">
                <copy>
                  <from>'BMG-002'</from>
                  <to>$g_Invoke_CommonLogging_PL_process_InputVariable.CommonLogging/ns11:MessageCode</to>
