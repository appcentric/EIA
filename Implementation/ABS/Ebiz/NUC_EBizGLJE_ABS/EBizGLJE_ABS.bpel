        <sequence name="CreateJournal_Sync">
          <assign name="setEHM">
            <copy>
              <from>xp20:current-dateTime()</from>
              <to>$EIA_HEADER_MESSAGE/ns3:ProcessStartTime</to>
            </copy>
            <copy>
              <from>ora:getCompositeName()</from>
