                </copy>
                <copy>
                  <from>1</from>
                  <to>$g_Invoke_CommonLogging_PL_process_InputVariable.CommonLogging/ns11:LogType</to>
                </copy>
                <copy>
                  <from>1</from>
                  <to>$g_Invoke_CommonLogging_PL_process_InputVariable.CommonLogging/ns11:LoggingSystem</to>
                </copy>
                <copy>
                  <from>'BMG-003'</from>
                  <to>$g_Invoke_CommonLogging_PL_process_InputVariable.CommonLogging/ns11:MessageCode</to>
                </copy>
                <copy>
                  <from>1</from>
                  <to>$g_Invoke_CommonLogging_PL_process_InputVariable.CommonLogging/ns11:TransactionStatus</to>
                </copy>
              </assign>
              <invoke name="Invoke_CommonLogging_PL" partnerLink="CommonLogging_PL" portType="ns11:CommonLogging"
