         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/Manish/NSNE_Bundle_EBSMTLTrxImp_ABF/DB_GetBundle_Details"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/Manish/NSNE_Bundle_EBSMTLTrxImp_ABF/DB_Update_Bundle"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/Manish/NSNE_Bundle_EBSMTLTrxImp_ABF/DB_EBS_Bundle_Dischange"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/Manish/NSNE_Bundle_EBSMTLTrxImp_ABF/DB_EBS_Bundle_ADJ"
