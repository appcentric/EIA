                              <xsd:element name="InvoiceTypeCode" type="xsd:string"/>
                              <xsd:element name="InvoiceTypeCodeName" type="xsd:string"/>
                              <xsd:element name="LastUpdatedDate" type="xsd:string"/>
                              <xsd:element name="MaterialAmt" type="xsd:double"/>
                              <xsd:element name="MiscChgAmt" type="xsd:double"/>
                              <xsd:element name="Remarks">
                                 <xsd:complexType>
                                    <xsd:attribute name="nil" type="xsd:string"/>
                                 </xsd:complexType>
                              </xsd:element>
                              <xsd:element name="ShipmentID" type="xsd:integer"/>
                              <xsd:element name="ShipmentLine" type="xsd:integer"/>
                              <xsd:element name="ShipmentVersionID" type="xsd:integer"/>
                              <xsd:element name="TotalAmt" type="xsd:double"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
         </xsd:sequence>
      </xsd:complexType>
   </xsd:element>
<xsd:annotation>
      <xsd:appinfo>NXSDSAMPLE=</xsd:appinfo>
      <xsd:appinfo>USEHEADER=false</xsd:appinfo>
   </xsd:annotation>
</xsd:schema>