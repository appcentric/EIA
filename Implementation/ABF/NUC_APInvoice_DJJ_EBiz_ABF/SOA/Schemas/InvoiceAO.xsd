                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="CallingClientName">
               <xsd:complexType>
                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="CallingClientRefID">
               <xsd:complexType>
                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="CallingDate">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="DateTime" type="xsd:string"/>
                     <xsd:element name="OffsetMinutes" type="xsd:integer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="CallingRequestedStartDate">
