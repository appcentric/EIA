            <xsd:element name="CallingAPIObject" type="xsd:string"/>
            <xsd:element name="CallingAckReq">
               <xsd:complexType>
                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="CallingClientID">
