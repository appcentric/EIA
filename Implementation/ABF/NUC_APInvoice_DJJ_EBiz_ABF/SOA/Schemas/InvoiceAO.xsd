                              <xsd:element name="DeadFrtAmt" type="xsd:double"/>
                              <xsd:element name="DiscountAmt" type="xsd:double"/>
                              <xsd:element name="DueDate" type="xsd:string"/>
                              <xsd:element name="EntityID" type="xsd:integer"/>
