               <xsd:complexType>
                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="SuccessCode" type="xsd:integer"/>
            <xsd:element name="ValidationMessages">
               <xsd:complexType>
                  <xsd:attribute name="nil" type="xsd:string"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="APInvoices">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="APInvoice" maxOccurs="unbounded">
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="AccountWgtRefID" type="xsd:integer"/>
                              <xsd:element name="AdjCode"/>
                              <xsd:element name="AdjCodeName"/>
